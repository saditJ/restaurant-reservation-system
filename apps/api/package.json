{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "prisma db seed --schema ../../prisma/schema.prisma",
    "dev": "nest start --watch",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "prisma:generate": "prisma generate --schema ../../prisma/schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema ../../prisma/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema ../../prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema ../../prisma/schema.prisma",
    "notifications:worker": "tsx src/workers/notifications.worker.ts",
    "webhooks:worker": "tsx src/workers/webhooks.worker.ts",
    "privacy:retention": "tsx src/workers/privacy.retention.worker.ts",
    "privacy:backfill": "tsx ../../scripts/privacy-backfill.ts",
    "openapi:generate": "tsx scripts/generate-openapi.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.0.2",
    "@prisma/client": "^6.17.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "ioredis": "^5.4.1",
    "nodemailer": "^6.9.15",
    "nestjs-pino": "^4.4.1",
    "pino": "^9.13.1",
    "pino-pretty": "^13.1.2",
    "reflect-metadata": "^0.2.2",
    "prom-client": "^15.1.1",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^11.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "prisma": "^6.17.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
  ,
  "prisma": {
    "seed": "tsx ../../prisma/seed.ts"
  }
}

